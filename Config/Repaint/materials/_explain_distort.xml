<?xml version="1.0" encoding="utf-8"?>
<Matparam xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
<!--
    DISTORTは屈折透過を実現するためのシェーダーです
-->
  <Shader>DISTORT</Shader>
  <Properties>
<!--
    Refraction ... 屈折率 1.0で屈折しない。シェーダー組込関数の都合上1.0未満がおすすめ
-->
    <Property><Name>_Refraction</Name><Type>Float</Type><Float>0.999</Float></Property>
  </Properties>
</Matparam>